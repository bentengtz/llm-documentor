{
    "llm": {
        "backend_type": "mock",
        "model_path": null,
        "server_url": null,
        "api_key": null,
        "model_name": null,
        "max_tokens": 1000,
        "temperature": 0.1,
        "timeout": 30,
        "retry_attempts": 3,
        "batch_size": 8,
        "context_window": 4096,
        "embedding_model": null
    },
    "database": {
        "db_path": "medical_analyzer.db",
        "backup_enabled": true,
        "backup_interval": 3600,
        "max_backups": 10
    },
    "export": {
        "default_format": "zip",
        "include_audit_log": true,
        "include_metadata": true,
        "compression_level": 6,
        "max_file_size": 104857600
    },
    "ui": {
        "theme": "default",
        "window_width": 1200,
        "window_height": 800,
        "auto_save": true,
        "auto_save_interval": 300,
        "show_tooltips": true,
        "confirm_exit": true
    },
    "analysis": {
        "max_chunk_size": 1000,
        "min_confidence": 0.5,
        "max_files_per_analysis": 1000,
        "supported_extensions": [".c", ".h", ".js", ".ts", ".jsx", ".tsx"],
        "enable_parallel_processing": true,
        "max_workers": 4
    },
    "logging": {
        "level": "INFO",
        "file_enabled": true,
        "console_enabled": true,
        "log_file": "medical_analyzer.log",
        "max_file_size": 10485760,
        "backup_count": 5,
        "format_string": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    },
    "custom": {
        "auto_update_check": true,
        "update_check_interval": 86400,
        "telemetry_enabled": false
    }
}